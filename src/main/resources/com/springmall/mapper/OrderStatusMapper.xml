<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springmall.mapper.OrderStatusMapper">

  <select id="queryStatusTextByStatusId" resultType="java.lang.String">
